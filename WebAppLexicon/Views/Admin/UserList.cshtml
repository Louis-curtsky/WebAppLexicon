@using Microsoft.AspNetCore.Identity
@model List<WebAppLexicon.Models.Identity.AppUser>
@{
    ViewData["Title"] = "UserList";
}

<h1>UserList</h1>

<table class="table table-warning table-striped">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th>Member ID</th>
            <th>Failed</th>
            <th>Actions</th>
        </tr>
        <tr>
            <th></th>
            <th>Account</th>
            <th>(Users Only)</th>
            <th>Login</th>
            <th></th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@item.MemberId</td>
                <td>@item.AccessFailedCount</td>
                <td>
                    @*<a asp-action="Details" asp-controller="Admin" asp-route-id="@item.Id">Details</a>*@
                </td>
            </tr>
        }
    </tbody>

</table>